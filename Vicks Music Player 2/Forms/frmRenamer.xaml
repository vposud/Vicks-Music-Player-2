<Window x:Class="Vicks_Music_Player_2.Forms.frmRenamer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vicks_Music_Player_2.Forms"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        Title="Mass Editor" Height="161" Width="240" WindowStartupLocation="CenterScreen" Background="Black" ResizeMode="CanMinimize" Icon="/Icons/icon.ico" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="listBox" Height="63" VerticalAlignment="Top" Background="Black" Foreground="White" AllowDrop="True" DragEnter="listBox_DragEnter" Drop="listBox_Drop" Margin="0,1,0,0"/>
        <ListBox x:Name="lstAlbumArt" Background="Black" Foreground="White" AllowDrop="True" DragEnter="lstAlbumArt_DragEnter" Drop="lstAlbumArt_Drop" HorizontalAlignment="Left" Width="115" Margin="0,68,0,0" Height="64" VerticalAlignment="Top"/>
        <ListBox x:Name="lstAlbumArtRemove" Background="Black" Foreground="White" AllowDrop="True" DragEnter="lstAlbumArtRemove_DragEnter" Drop="lstAlbumArtRemove_Drop" HorizontalAlignment="Left" Width="116" Margin="118,68,0,0" Height="64" VerticalAlignment="Top"/>
    </Grid>
</Window>
